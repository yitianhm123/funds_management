    </sequenceFlow>
    <sequenceFlow id="flow13" name="拒绝" sourceRef="exclusivegateway3" targetRef="endGycPayApply">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${result=='0'}]]></conditionExpression>
